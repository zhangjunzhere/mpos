<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.itertk.app.mpos.account.AccountHomeActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/shop_config_selector"
                android:id="@+id/btnAccountCenter"
                android:gravity="center_vertical"
                android:clickable="true">

                <ImageView
                    android:duplicateParentState="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/usercenter_config_image_selector"
                    android:id="@+id/imageShop" />

                <TextView
                    android:duplicateParentState="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30sp"
                    android:id="@+id/textShop"
                    android:textColor="@drawable/shop_config_text_selector"
                    android:text="我的账户" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="-1dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/update_config_selector"
                android:id="@+id/btnAccountLevel"
                android:gravity="center_vertical"
                android:visibility="gone"
                android:clickable="true">

                <ImageView
                    android:duplicateParentState="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/user_level_config_image_selector" />

                <TextView
                    android:duplicateParentState="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30sp"
                    android:textColor="@drawable/shop_config_text_selector"
                    android:text="账户等级" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="-1dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/update_config_selector"
                android:id="@+id/btnCash"
                android:gravity="center_vertical"
                android:clickable="true">

                <ImageView
                    android:duplicateParentState="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/cash_config_image_selector"
                    android:id="@+id/imageUpdate" />

                <TextView
                    android:duplicateParentState="true"
                    android:layout_marginLeft="20px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30sp"
                    android:id="@+id/textJCGX"
                    android:textColor="@drawable/shop_config_text_selector"
                    android:text="我要提现" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="-1dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/update_config_selector"
                android:id="@+id/btnSaleRecord"
                android:gravity="center_vertical"
                android:clickable="true">

                <ImageView
                    android:duplicateParentState="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bill_order_image_selector"
                    android:id="@+id/imageOrder" />

                <TextView
                    android:duplicateParentState="true"
                    android:layout_marginLeft="20px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30sp"
                    android:id="@+id/textOrder"
                    android:textColor="@drawable/shop_config_text_selector"
                    android:text="销售记录" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="-1dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/update_config_selector"
                android:id="@+id/btnSaleStatistic"
                android:gravity="center_vertical"
                android:clickable="true">

                <ImageView
                    android:duplicateParentState="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bill_sale_info_image_selector"
                    android:id="@+id/imageSaleStatistic" />

                <TextView
                    android:duplicateParentState="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30dp"
                    android:id="@+id/textSaleStatistic"
                    android:textColor="@drawable/shop_config_text_selector"
                    android:text="销售统计" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="-1dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/update_config_selector"
                android:id="@+id/btnProductStatistic"
                android:gravity="center_vertical"
                android:clickable="true">

                <ImageView
                    android:duplicateParentState="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bill_product_sale_info_image_selector"
                    android:id="@+id/imageProductStatistic" />

                <TextView
                    android:duplicateParentState="true"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30dp"
                    android:id="@+id/textProductStatistic"
                    android:textColor="@drawable/shop_config_text_selector"
                    android:text="商品统计" />

            </LinearLayout>




            <RelativeLayout
                android:layout_marginTop="-1dp"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/set_bottom"
                android:gravity="center_vertical"
                android:clickable="true"></RelativeLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_marginLeft="-1dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/set_container"
            android:background="@drawable/right_corner_eara"></RelativeLayout>

    </LinearLayout>

</RelativeLayout>
